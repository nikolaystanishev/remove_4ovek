{
    "dataset": {
        "url": "http://datasets.d2.mpi-inf.mpg.de/andriluka_cvpr10/cvpr10_multiview_pedestrians.tar.gz",
        "path": "./dataset",
        "folder": "./dataset/cvpr10_multiview_pedestrians",
        "dataset_images": {
            "train_folder": "./dataset/cvpr10_multiview_pedestrians/train",
            "validation_folder": "./dataset/cvpr10_multiview_pedestrians/validate",
            "test_folder": "./dataset/cvpr10_multiview_pedestrians/test"
        },
        "dataset_annotations": {
            "train_annotations": [
                "./dataset/cvpr10_multiview_pedestrians/viewpoints_train1.al",
                "./dataset/cvpr10_multiview_pedestrians/viewpoints_train2.al",
                "./dataset/cvpr10_multiview_pedestrians/viewpoints_train3.al",
                "./dataset/cvpr10_multiview_pedestrians/viewpoints_train4.al",
                "./dataset/cvpr10_multiview_pedestrians/viewpoints_train5.al",
                "./dataset/cvpr10_multiview_pedestrians/viewpoints_train6.al",
                "./dataset/cvpr10_multiview_pedestrians/viewpoints_train7.al",
                "./dataset/cvpr10_multiview_pedestrians/viewpoints_train8.al"
            ],
            "validation_annotations": [
                "./dataset/cvpr10_multiview_pedestrians/viewpoints_validate.al"
            ],
            "test_annotations": [
                "./dataset/cvpr10_multiview_pedestrians/viewpoints_test.al"
            ]
        },
        "pickle_name": {
            "train": "./dataset/train_data.pickle",
            "validation": "./dataset/validation_data.pickle",
            "test": "./dataset/test_data.pickle"
        }
    },

    "image_info": {
        "image_size": 288,
        "pixel_depth": 255.0,
        "color_channels": 3
    },

    "label_info": {
        "grid_size": 9,
        "number_of_annotations": 4
    },

    "network": {
        "model_binary_data_file": "./model.h5",
        "results_file": "./results.txt",
        "json_model_structure": "./model.json",
        "train": {
            "batch_size": 32,
            "number_of_epochs": 200,
            "loss": {
                "alpha_coord": 5,
                "alpha_noobj": 0.5
            },
            "optimizer": {
                "learning_rate": 0.0001,
                "momentum": 0.9,
                "decay": 0.0005
            }
        },
        "predict": {
            "min_probability": 0.5
        }
    }
}